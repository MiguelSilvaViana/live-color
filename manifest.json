{
  "manifest_version": 3,
  "name": "pick color",
  "version": "1.0",
  "description": "Uma extensão que captura a cor da área onde o mouse está posicionado",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    }
  },
  "icons": {
    "16": "icons/icon16.jpg",
    "48": "icons/icon48.jpg",
    "128": "icons/icon128.jpg"
  },
  "author": "miguelsilvavianaa@gmail.com"
}
